{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Rust",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../target/debug/subscriber",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cargo build",
        },
        {
            "name": "Debug Rust Tests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../target/debug/deps/subscriber-${command:rust-analyzer.getTargetLayoutHash}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cargo test build"
        }
    ]
}